{
  "name": "koa-universal-react-redux",
  "version": "1.0.0",
  "description": "koa-universal-react-redux",
  "scripts": {
    "clean": "rimraf build && mkdirp build/server build/client build/shared",
    "build-shared": "babel -q -d build/shared src/shared -s",
    "build-server": "babel -q -d build/server src/server -s",
    "build": "npm run clean && npm run build-server && npm run build-shared",
    "dev": "npm run build && nodemon -q -x babel-node src/server/app.js",
    "prod": "npm run build && webpack --colors && node build/server/app.js"
  },
  "keywords": [
    "koa",
    "react",
    "redux",
    "universal",
    "isomorphic"
  ],
  "author": "Boris Ryabov",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.9.1",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-node5": "^1.1.1",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-react": "^3.15.0",
    "history": "^1.17.0",
    "if-loader": "^1.0.1",
    "immutable": "^3.7.6",
    "koa": "^2.0.0-alpha.3",
    "koa-66": "^0.8.4",
    "koa-body": "^1.4.0",
    "koa-compose": "^2.3.0",
    "koa-convert": "^1.2.0",
    "koa-etag": "^2.1.0",
    "koa-helmet": "^2.0.0-alpha.1",
    "koa-response-time": "^1.0.2",
    "koa-static-cache": "^3.1.4",
    "lodash": "^4.0.1",
    "mkdirp": "^0.5.1",
    "progress-bar-webpack-plugin": "^1.3.0",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-immutable-proptypes": "^1.5.1",
    "react-redux": "^4.0.6",
    "react-router": "^2.0.0-rc5",
    "react-router-redux": "^2.1.0",
    "redux": "^3.0.5",
    "redux-actions": "^0.9.1",
    "redux-immutablejs": "0.0.8",
    "redux-optimist-promise": "^0.6.0",
    "reselect": "^2.0.2",
    "rimraf": "^2.5.1",
    "webpack": "^1.12.12",
    "webpack-hot-middleware": "^2.6.3",
    "winston": "^2.1.1",
    "koa-webpack-dev-middleware": "^1.1.0",
    "koa-webpack-hot-middleware": "^1.0.3"
  },
  "devDependencies": {
    "babel-plugin-react-transform": "^2.0.0",
    "nodemon": "^1.8.1",
    "react-transform-catch-errors": "^1.0.1",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.0",
    "webpack-module-hot-accept": "^1.0.4",
    "redux-devtools": "^3.0.2",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.2"
  }
}
