{
  "name": "koa-universal-react-redux",
  "version": "1.0.0",
  "description": "koa-universal-react-redux",
  "scripts": {
    "clean": "rimraf build && mkdirp build/server build/client build/shared",
    "build-shared": "babel -q -d build/shared src/shared -s",
    "build-server": "babel -q -d build/server src/server -s",
    "build": "npm run clean && npm run build-server && npm run build-shared",
    "dev": "npm run build && nodemon -q -x babel-node src/server/app.js",
    "prod": "npm run build && webpack --colors && node build/server/app.js"
  },
  "keywords": [
    "koa",
    "react",
    "redux",
    "universal",
    "isomorphic"
  ],
  "author": "Boris Ryabov",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.9.1",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-node5": "^1.2.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "eslint": "^2.5.3",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.2.3",
    "history": "^2.0.1",
    "if-loader": "^1.0.1",
    "immutable": "^3.7.6",
    "koa": "^2.0.0-alpha.3",
    "koa-66": "^0.8.4",
    "koa-body": "^1.4.0",
    "koa-compose": "^2.4.0",
    "koa-convert": "^1.2.0",
    "koa-etag": "^2.1.1",
    "koa-helmet": "^2.0.0-alpha.1",
    "koa-response-time": "^1.0.2",
    "koa-static-cache": "^3.1.6",
    "koa-webpack-dev-middleware": "^1.2.0",
    "koa-webpack-hot-middleware": "^1.0.3",
    "lodash": "^4.7.0",
    "mkdirp": "^0.5.1",
    "progress-bar-webpack-plugin": "^1.6.0",
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-immutable-proptypes": "^1.7.0",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.1",
    "redux-actions": "^0.9.1",
    "redux-immutablejs": "0.0.8",
    "redux-optimist-promise": "^0.7.1",
    "reselect": "^2.2.1",
    "rimraf": "^2.5.2",
    "webpack": "^1.12.14",
    "webpack-hot-middleware": "^2.10.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "babel-plugin-react-transform": "^2.0.0",
    "nodemon": "^1.8.1",
    "react-transform-catch-errors": "^1.0.1",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.0",
    "webpack-module-hot-accept": "^1.0.4",
    "redux-devtools": "^3.0.2",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.2"
  }
